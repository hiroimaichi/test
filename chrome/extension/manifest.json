{
	"name": "Search Atnd",
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"version": "1.0",
	"description": "you can search atnd events with key words, date(year, month).",
	"browser_action":{
	"default_icon":"icon.png",
	"default_popup": "search_atnd.html"
	},
	"permissions": [
	"http://api.atnd.org/"
	]
}
